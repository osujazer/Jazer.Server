{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "MassTransit": "Warning",
      "Discord": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "MassTransit": "Warning",
        "Discord": "Warning"
      }
    },
    "Properties": {
      "application": "Jazer.Server"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },

  "AllowedHosts": "*",

  "ServiceHost": "localhost",
  "ServicePort": 7272,
  "JwtSecretKey": "some-super-secret-jwt-secret-key",
  "JwtExpiryMinutes": 60,
  "JwtIssuer": "osujazer.com",
  "JwtAudience": "some-audience",
  "DatabaseConnectionString": "Server=localhost"
}
